{
  "name": "attlaz/attlaz-monolog",
  "description": "Attlaz integration for Monolog",
  "keywords": [
    "log",
    "logging",
    "monolog",
    "attlaz"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Stijn Duynslaeger",
      "email": "hello@attlaz.com",
      "homepage": "https://attlaz.com"
    }
  ],
  "require": {
    "php": ">=7.4 | >=8.0",
    "monolog/monolog": "^2.0",
    "attlaz/client": "^1.7.7"
  },
  "require-dev": {
    "ext-json": "*",
    "phpunit/phpunit": "^8 | ^9",
    "inpsyde/php-coding-standards": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Attlaz\\AttlazMonolog\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Attlaz\\AttlazMonolog\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
